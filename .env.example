# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Solana Configuration
# Set SOLANA_NETWORK to "devnet" to use devnet, otherwise mainnet is used
SOLANA_NETWORK=devnet
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_RPC_URL_DEVNET=https://api.devnet.solana.com

# Database (PostgreSQL)
# For local development, you can use SQLite by changing provider in schema.prisma to "sqlite"
# and setting: DATABASE_URL="file:./data.db"
DATABASE_URL=postgresql://user:password@localhost:5432/mydb

# Encryption Key (IMPORTANT: Generate a random 64-character hex string and keep it secret!)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
# WARNING: If you change this after wallets are created, existing wallets cannot be decrypted!
ENCRYPTION_KEY=your-encryption-key-here-generate-with-node-randomBytes

# Jupiter Ultra Swap API
# Get API key from https://portal.jup.ag/
JUPITER_API_URL=https://api.jup.ag/ultra/v1
JUPITER_API_KEY=your-api-key-here

# Optional: RPC endpoint with better rate limits (e.g., Helius, QuickNode)
# SOLANA_RPC_URL=https://your-rpc-endpoint.com

# Webhook URL for Vercel deployment (set this after deploying)
# Get your Vercel URL from deployment dashboard
# WEBHOOK_URL=https://your-app.vercel.app/api/webhook

